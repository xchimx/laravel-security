{
    "name": "xchimx/laravel-security",
    "description": "A Laravel package for automated monitoring of security vulnerabilities and outdated packages in Composer and NPM dependencies.",
    "keywords": [
        "xchimx",
        "laravel",
        "laravel-security",
        "laravel",
        "security",
        "audit",
        "composer",
        "npm",
        "vulnerability"
    ],
    "homepage": "https://github.com/xchimx/laravel-security",
    "license": "MIT",
    "authors": [
        {
            "name": "Tobias Schottst√§dt",
            "email": "code@schottstaedt.net",
            "homepage": "https://www.schottstaedt.net",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^10.0",
        "illuminate/support": "^10.0",
        "laravel/slack-notification-channel": "^3.0",
        "spatie/laravel-package-tools": "^1.16"
    },
    "require-dev": {
        "larastan/larastan": "^3.0",
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^7.0",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpstan/phpstan-phpunit": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Xchimx\\LaravelSecurity\\": "src/",
            "Xchimx\\LaravelSecurity\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Xchimx\\LaravelSecurity\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Xchimx\\LaravelSecurity\\LaravelSecurityServiceProvider"
            ],
            "aliases": {
                "LaravelSecurity": "Xchimx\\LaravelSecurity\\Facades\\LaravelSecurity"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
